@import "../../wxSearchView/wxSearchView.wxss";

.head-menu{
  font-size: 25rpx;
  line-height: 90rpx;
  position: relative;
  display: flex;
  height: 90rpx;
}
.menu-item{
  width: 16.66%;
  justify-items: center;
  align-items: center;
  text-align: center;
  margin: 2rpx;
  background-color: white;
}
.menu-warp {
  width: 100%;
  background: #fff;
  background: rgba(255, 255, 255, 0.95);
  margin-top: 2rpx;
  font-size: 40rpx;
  line-height: 120rpx;
  position: relative;
  display: flex;
  justify-items: center;
}

.menu-warp .left {
  text-align: left;
  font-weight: bold;

  width: 50%;
  padding-left: 20rpx;
}

.menu-warp .right {
  width: 50%;
  text-align: right;
  padding-right: 20rpx;
}

.menu-warp .right text {
  width: 100%;
  text-align: center;
}