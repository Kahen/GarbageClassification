page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.title{
 text-align: center;
 padding-top: 20%;
}
@font-face {
  font-family: 'webfont';
  font-display: swap;
  src: url('//at.alicdn.com/t/webfont_h0avlml8nx4.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_h0avlml8nx4.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/webfont_h0avlml8nx4.woff2') format('woff2'),
  url('//at.alicdn.com/t/webfont_h0avlml8nx4.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/webfont_h0avlml8nx4.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/webfont_h0avlml8nx4.svg#AlibabaPuHuiTiH') format('svg'); /* iOS 4.1- */
}
.web-font1 {
  font-family: "webfont" !important;
  font-size: 66rpx;
  font-style: normal;
  color: #5186ec;
